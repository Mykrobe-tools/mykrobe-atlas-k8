{
  "apiVersion": "apps/v1beta2",
  "kind": "Deployment",
  "metadata": {
    "labels": {
      "app": "atlas"
    },
    "name": "atlas-deployment",
    "namespace": "default"
  },
  "spec": {
    "selector": {
      "matchLabels": {
        "app": "atlas"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "atlas"
        }
      },
      "spec": {
        "containers": [
          {
            "image": "makeandship/atlas-client:29",
            "name": "atlas",
            "ports": [
              {
                "containerPort": 3000,
                "protocol": "TCP"
              }
            ],
            "env": [
              
            ]
          }
        ],
        "imagePullSecrets": [{ "name": "dockerhub" }]
      }
    }
  }
}
